      <trans-unit id="chiSonoSubtitle" datatype="html">
        <source> Sono una make-up artist e hair stylist con una passione profonda per il mondo della bellezza e della creatività. </source>
        <context-group purpose="location">
          <context context-type="sourcefile">src/app/pages/chi-sono/chi-sono.component.html</context>
