{
  "name": "azzurra-makeup-deploy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",

    "build:ssr": "ng build && ng run azzurra-makeup-deploy:server",
    "build:ssr:prod": "ng build --configuration production && ng run azzurra-makeup-deploy:server:production",
    
    "serve:ssr:dev": "cross-env BACKEND_URL=http://localhost:4000 node dist/azzurra-makeup-deploy/server/main.js",
    "start:ssr:dev": "npm run build:ssr && npm run serve:ssr:dev",

    "serve:ssr:prod": "cross-env NODE_ENV=production node dist/azzurra-makeup-deploy/server/main.js",
    "start:ssr:prod": "npm run build:ssr:prod && npm run serve:ssr:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/platform-server": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@angular/ssr": "^18.2.20",
    "express": "^4.18.2",
    "http-proxy-middleware": "^3.0.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.20",
    "@angular/cli": "^18.2.20",
    "@angular/compiler-cli": "^18.2.0",
    "@angular/localize": "^18.2.13",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "cross-env": "^10.0.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.2"
  }
}